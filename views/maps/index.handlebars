<div id="map"></div>
<script>
    function initMap() {
        var myLatLng = { lat:{{lat}}, lng:{{lng}} };

        // Create a map object and specify the DOM element for display.
        var map = new google.maps.Map(document.getElementById('map'), {
            center: myLatLng,
            zoom: 4
        });

        // Create a marker and set its position.
        var marker = new google.maps.Marker({
            map: map,
            position: myLatLng,
            title: 'everlastingWardrobe!'
        });
    }

</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCyX5XZTCYMVUvb6q0mXax2Hp4ibcmXURY&callback=initMap"
        async defer></script>